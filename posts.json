---
layout: null
permalink: posts.json
---
[
    {% for post in site.posts limit:10 %}
      {
        "title": "{{ post.title | replace: '"', '\"' }}",
        "date": "{{ post.date }}",
        "excerpt": "{{ post.desc | replace: '"', '\"' | strip_newlines | strip_html }}",
        "url": "{{ post.url | prepend: site.url }}",
      }{% if forloop.last != true %},{% endif %}
    {% endfor %}
]